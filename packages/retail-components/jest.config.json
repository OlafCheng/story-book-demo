{
  "globals": {
    "ts-jest": {
      "babelConfig": {
        "presets": [
          "es2015"
        ]
      }
    }
  },
  "verbose": true,
  "collectCoverage": true,
  "coverageReporters": [
    "text",
    "text-summary",
    "lcov",
    "html"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 30,
      "branches": 30,
      "functions": 30,
      "lines": 30
    }
  },
  "timers": "fake",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json"
  ],
  "transform": {
    "^.+\\.js?$": "babel-jest",
    "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleNameMapper": {
    "^tab-select(.*)$": "<rootDir>/src/tab-select$1"
  }
}