{
  "verbose": true,
  "collectCoverage": true,
  "coverageReporters": [
    "text",
    "text-summary",
    "lcov",
    "html"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 30,
      "branches": 30,
      "functions": 30,
      "lines": 30
    }
  },
  "timers": "fake",
  "moduleFileExtensions": [
    "jsx",
    "ts",
    "tsx",
    "js",
    "json"
  ],
  "transform": {
    "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
  },
  "testMatch": [
    "**/__tests__/*.(ts|tsx|jsx|js)"
  ],
  "moduleNameMapper": {
    "^tab-select(.*)$": "<rootDir>/src/tab-select$1"
  }
}